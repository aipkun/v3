#!/bin/bash
REPO="https://raw.githubusercontent.com/aipkun/v3/main/"
TIME="10"
#CHATID=$(grep -E "^#bot# " "/etc/bot/.bot.db" | cut -d ' ' -f 3)
#KEY=$(grep -E "^#bot# " "/etc/bot/.bot.db" | cut -d ' ' -f 2)
CHATID="6830313714"
KEY="6794400481:AAHAl_P5DvPJPYOMm7L8Sz5KeHCsUPrvesk"
URL="https://api.telegram.org/bot$KEY/sendMessage"
TEXT="
<b>NOTIF UPDATESC</b>
<i>AIP TUNNEL</i>
"
curl -s --max-time 10 -d "chat_id=$CHATID&disable_web_page_preview=1&text=$TEXT&parse_mode=html" $URL >/dev/null

function ins_menu() {
cd
wget -q -O menu.zip "${REPO}bin/menu.zip"
unzip menu.zip
chmod +x menu/*
mv menu/* /usr/local/sbin
rm -rf menu
rm -rf menu.zip
}
function xdxl_hunter() {
cd
wget -q -O user-limit.sh "${REPO}limit/user-limit.sh"
chmod +x user-limit.sh
bash user-limit.sh
rm -fr user-limit.sh
}
function install() {
ins_menu
xdxl_hunter
}
install;clear;menu
